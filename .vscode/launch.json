{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [

        {
            "name": "React: Node",
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "${workspaceRoot}/aa-front-end/node_modules/.bin/react-scripts",
            "args": [
                "test",
                "--runInBand",
                "--no-cache",
                "--watchAll=false"
            ],
            "cwd": "${workspaceRoot}/aa-front-end/",
            "protocol": "inspector",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
        },
        {
            "name": "React: Chrome",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:3000",
            "webRoot": "${workspaceRoot}/aa-front-end/src/",
            "sourceMapPathOverrides": {
                "webpack:///./*": "${webRoot}/*",
            }
        },    
        {
            "name": "Launch Chrome DevTools",
            "type": "devtools-for-chrome",
            "request": "launch",
            "file": "${workspaceFolder}/index.html"
        },
        {
            "name": "Attach Chrome DevTools",
            "type": "devtools-for-chrome",
            "request": "attach",
            "url": "http://localhost:9000/"
        },
        {
            "name": "Launch Program",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/aa-front-end/src/playground.js",
        },
        {
            "name":"Python: Django",
            "type":"python",
            "request":"launch",
            "program":"${workspaceFolder}/manage.py",
            "args":["runserver","--noreload","--nothreading"],
            "django":true
        },
    ]
}
