[run]
source = .
branch = True

omit =
  .venv/*
  .pytest_cache/*,
  .hypothesis/*,
  *apps.py,
  *migrations/*,
  *aa_project/pytest.py,
  */node_modules/*,
  */static/*,
  */staticfiles/*,
  */tests/test*.py,
  */users/signals.py,
  *admin.py*,
  *wsgi.py,
  *asgi.py,
  manage.py,
  *feeds.py,
  *__init__.py,
  *__pycache__,
  */site-packages/*,
  */distutils/*,
  *sitemap.py,

[report]

show_missing = True

exclude_lines =
  pragma: no cover
  def __repr__
  if settings.DEBUG:
  if self.debug:
  if DEBUG:
  if not DEBUG:
  raise AssertionError
  raise NotImplementedError
  if 0:
  if __name__ == .__main__.:
